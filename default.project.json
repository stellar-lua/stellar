{
    "name": "Stellar Project",

    "tree": {
        "$className": "DataModel",
        "$ignoreUnknownInstances": true,

        "HttpService": {
            "$className": "HttpService",
            "$properties": {
                "HttpEnabled": true
            }
        },

        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",

            "ClientModules": {
                "$className": "Folder",
                "$path": "src/Core/Client"
            },

            "SharedModules": {
                "$className": "Folder",
                "$path": "src/Core/Shared"
            },

            "Packages": {
                "$className": "Folder",
                "$path": "Packages"
            }
        },

        "ServerScriptService": {
            "$className": "ServerScriptService",
            "$path": "src/Scripts/Server"
        },

        "ServerStorage": {
            "$className": "ServerStorage",

            "ServerModules": {
                "$className": "Folder",
                "$path": "src/Core/Server"
            },

            "Packages": {
                "$className": "Folder",
                "$path": "ServerPackages"
            },

            "Commands": {
                "$className": "Folder",
                "$path": "src/Core/Commands"
            }
        },

        "StarterPlayer": {
            "$className": "StarterPlayer",
            "StarterPlayerScripts": {
                "$className": "StarterPlayerScripts",
                "$path": "src/Scripts/Client"
            }
        }
    }
}
